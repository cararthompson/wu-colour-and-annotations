y = "Time in the oven") +
theme(panel.grid = element_blank())
duration_plot
duration_plot +
scale_x_discrete(breaks = c("Adelie", "Chinstrap", "Gentoo"),
labels = c("A = Adelie", "B = Chinstrap", "C = Gentoo")) +
coord_flip()
duration_plot +
scale_x_discrete(breaks = c( "Chinstrap", "Adelie","Gentoo"),
labels = c("A = Adelie", "B = Chinstrap", "C = Gentoo")) +
coord_flip()
duration_plot +
scale_x_discrete(breaks = c("Gentoo", "Chinstrap", "Adelie"),
labels = c("A = Adelie", "B = Chinstrap", "C = Gentoo")) +
coord_flip()
duration_plot +
scale_x_discrete(breaks = c("Adelie", "Chinstrap", "Gentoo"),
labels = c("A = Adelie", "B = Chinstrap", "C = Gentoo")) +
coord_flip()
duration_plot +
scale_x_discrete(breaks = c("Adelie", "Chinstrap", "Gentoo"),
labels = c("A = Adelie", "B = Chinstrap", "C = Gentoo"),
limits = "rev") +
coord_flip()
duration_plot +
scale_x_discrete(breaks = c("Adelie", "Chinstrap", "Gentoo"),
